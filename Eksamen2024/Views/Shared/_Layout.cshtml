<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Eksamen2024</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Eksamen2024.styles.css" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
</head>
<body>
    
  <div class="container-fluid">
    <!-- Sidebar -->
    <div class="col-md-3" id="sidebar">
        <ul class="list-group">
            <div id= meny-ikon>
                <li class="list-group-item">
                    <i class="fas fa-bars"></i>
                    <a href="#">Meny</a></li>
            </div>
            <div id= lagret-ikon>
                <i class="fas fa-bookmark"></i>     
                <li class="list-group-item"><a href="#">Lagret</a></li>
            </div>       
        </ul>
        
        <!-- "Logg inn"-knappen som skal plasseres nederst -->
        <div id="login-link">
            <button class="btn btn-primary">Logg inn</button>
        </div>
    </div>

    <!-- Main Content (Kartet) -->
    <div class="col-md-9" id="main-content">
        <div id="map"> @RenderBody()</div> <!-- Kartet må være her -->
    </div>
</div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
